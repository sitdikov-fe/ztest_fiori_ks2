{
	"_version": "1.7.0",
	"sap.app": {
		"id": "ztest_fiori_ks",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponent",
			"version": "0.0.0"
		},
		"crossNavigation": {
			"inbounds": {}
		},
		"dataSources": {
			"ZTEST_FIORI_KOSI_SRV": {
				"uri": "/sap/opu/odata/sap/ZTEST_FIORI_KOSI_SRV/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/ZTEST_FIORI_KOSI_SRV/metadata.xml",
					"annotations": []
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": ["sap_hcb", "sap_belize"]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "ztest_fiori_ks.view.Root",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {},
				"sap.ushell": {},
				"sap.collaboration": {},
				"sap.ui.comp": {},
				"sap.uxap": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "ztest_fiori_ks.i18n.i18n"
				}
			},
			"": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "OneTime",
					"defaultCountMode": "Inline"
				},
				"dataSource": "ZTEST_FIORI_KOSI_SRV",
				"preload": true
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"path": "ztest_fiori_ks.view",
				"controlId": "rootControl",
				"controlAggregation": "pages",
				"type": "View",
				"viewType": "XML",
				"async": true,
				"bypassed": {
					"target": []
				}
			},
			"routes": [{
				"name": "page1",
				"pattern": "",
				"titleTarget": "",
				"greedy": true,
				"target": ["page1"]
			}, {
				"name": "page2",
				"pattern": "CreateOrder",
				"titleTarget": "",
				"greedy": false,
				"target": ["page2"]
			}, {
				"name": "page3",
				"pattern": "Order",
				"titleTarget": "",
				"greedy": false,
				"target": ["page3"]
			}, {
				"name": "page4",
				"pattern": "ViewOrder",
				"titleTarget": "",
				"greedy": false,
				"target": ["page4"]
			}, {
				"name": "page5",
				"pattern": "View",
				"titleTarget": "",
				"greedy": false,
				"target": ["page5"]
			},
			{
				"name": "page6",
				"pattern": "ViewTable",
				"titleTarget": "",
				"greedy": false,
				"target": ["page6"]
			}],
			"targets": {
				"page1": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "first",
					"viewLevel": 1
				},
				"page2": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "Create01",
					"viewLevel": 2
				},
				"page3": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "Create02",
					"viewLevel": 3
				},
				"page4": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "Order01",
					"viewLevel": 2
				},
				"page5": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "Order02",
					"viewLevel": 3
				},
				"page6": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "SmartTest",
					"viewLevel": 2
				}
			}
		}
	},
	"sap.platform.abap": {
		"uri": "/sap/bc/ui5_ui5/sap/zbsp_uitestkosi/webapp",
		"_version": "1.1.0"
	}
}